						      "quantity": false
						    },
						    "styles": {
						      "product": {
						        "@media (min-width: 601px)": {
						          "max-width": "100%",
						          "margin-left": "0",
						          "margin-bottom": "50px"
						        }
						      },
						      "compareAt": {
						        "font-size": "12px"
						      }
						    }
						  },
						  "cart": {
						    "contents": {
						      "button": true
						    },
						    "styles": {
						      "footer": {
						        "background-color": "#fff"
						      }
						    }
						  },
						  "modalProduct": {
						    "contents": {
						      "img": true,
						      "imgWithCarousel": true,
						      "variantTitle": false,
						      "buttonWithQuantity": true,
						      "button": false,
						      "quantity": false
						    },
						    "styles": {
						      "product": {
						        "@media (min-width: 601px)": {
