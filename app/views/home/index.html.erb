						      apiKey: 'dac21f19e3dbb52e6b15c6cd24554732',
						      appId: '6',
						    });

						    ShopifyBuy.UI.onReady(client).then(function (ui) {
						      ui.createComponent('product', {
						        id: [8818884613],
						        node: document.getElementById('product-component-b4eda83a8fa'),
						        moneyFormat: '%24%7B%7Bamount%7D%7D',
						        options: {
						  "product": {
						    "variantId": "all",
						    "width": "240px",
						    "contents": {
						      "imgWithCarousel": false,
